    "### Group Anagrams"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "629b61cb",
   "metadata": {},
   "outputs": [],
   "source": [
    "strs = [\"eat\",\"tea\",\"tan\",\"ate\",\"nat\",\"bat\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "45e85085",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[['eat', 'tea', 'ate'], ['tan', 'nat'], ['bat']]\n"
     ]
    }
   ],
   "source": [
    "from collections import defaultdict\n",
    "\n",
    "my_dict = defaultdict(list)\n",
    "\n",
    "for word in strs:\n",
    "    key = ''.join(sorted(word))\n",
    "    my_dict[key].append(word)\n",
    "\n",
    "wassup = [word for word in my_dict.values()]\n",
    "print(wassup)"
   ]
